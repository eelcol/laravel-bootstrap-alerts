{
    "name": "eelcol/laravel-bootstrap-alerts",
    "description": "Easily implement alert messages in your Bootstrap design using Laravel.",
    "keywords": [
        "laravel",
        "bootstrap",
        "alerts"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Eelco Luurtsema",
            "email": "eelcoluurtsema@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Eelcol\\LaravelBootstrapAlerts\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Eelcol\\LaravelBootstrapAlerts\\AlertsServiceProvider"
            ]
        }
    }
}
